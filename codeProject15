#include <iostream>
#include <string>
#include <fstream>
#include <iomanip>

using namespace std;

int main()
{
  string pass = "";

  cout<<"put it in"<<endl;
  getline(cin, pass);

  if(pass == "abc$123")
  {
    cout<<"Hello master Bruce"<<endl;
  }
  else
  {
    cout<<"Error, Intruder alert"<<endl;
  }


  return 0;
}
